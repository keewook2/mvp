{
  "compilerOptions": {
    "outDir": "./client/dist/",
    "noImplicitAny": true,
    "module": "CommonJS",
    "target": "es5",
    "jsx": "react-jsx",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "lib": ["es6", "dom", "es5"],
  },
}